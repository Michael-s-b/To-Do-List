<%- include("header") %>
<div class="box" id="heading">
	<h1><%= listTitle %></h1>
</div>

<div class="box">
	<% for( let i=0; i < listOfItems.length; i++ ) { %>
	<div class="item">
		<input type="checkbox" />
		<p><%= listOfItems[i] %></p>
	</div>
	<% } %>
	<form class="item" action="<%= formAction %>" method="post">
		<input
			autocomplete="off"
			placeholder="New Item"
			type="text"
			name="newItem" />
		<button name="button" type="submit">+</button>
	</form>
</div>
<%- include("footer") %>
